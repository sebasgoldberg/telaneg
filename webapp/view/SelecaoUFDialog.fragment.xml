<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:form="sap.ui.layout.form">

	<SelectDialog
        id="selecaoUFDialog"
        contentWidth="400px"
        contentHeight="500px"
        growing="true"
        growingThreshold="30"
        growingScrollToLoad="true"
        multiSelect="true"
        rememberSelections="false"
		noDataText="UFs nÃ£o encontradas."
		title="Selecionar UFs"
		search="onSearch"
		confirm="onSelecionar"
		cancel="onFechar"
		items="{
			path: 'bandeira/ufs'
		}" >
		<StandardListItem
			title="{ID}"
			description=""
			type="Active" />
	</SelectDialog>

</core:FragmentDefinition>





