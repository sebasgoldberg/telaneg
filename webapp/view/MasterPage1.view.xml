                        <secondStatus>
                            <ObjectStatus title="IC (%)" text="{path: 'NovoIC', type: 'sap.ui.model.type.Float', formatOptions: { maxFractionDigits: 2 }}" state="{StatusNovoIC}"/>
                        </secondStatus>
                    </ObjectListItem>
                </items>
