<mvc:View
    controllerName="simplifique.telaneg.controller.TaskDetail"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:uxap="sap.uxap"
    xmlns="sap.m"
    xmlns:layout="sap.ui.layout"
    xmlns:form="sap.ui.layout.form"
    xmlns:core="sap.ui.core">

    <uxap:ObjectPageLayout showTitleInHeaderContent="false" showHeaderContent="true" showEditHeaderButton="false" showFooter="true" class="sapUiSizeCompact">
        <uxap:headerTitle>
            <uxap:ObjectPageHeader objectTitle="{Descricao}" objectSubtitle="{ID}, Estipulação {Estipulacao}" isObjectIconAlwaysVisible="false" isObjectTitleAlwaysVisible="true" isObjectSubtitleAlwaysVisible="true" showPlaceholder="false">
                <uxap:navigationBar>
                    <Bar>
                        <!--
                        <contentLeft>
                            <Button type="Back" press="_onFioriObjectPageHeaderPress"/>
                        </contentLeft>
                        -->
                        <contentMiddle>
                            <Title text="{tipoNegociacao/Descricao}"/>
                        </contentMiddle>
                        <contentRight/>
                    </Bar>
                </uxap:navigationBar>
                <uxap:actions>
                    <Button text="Anexar" icon="sap-icon://attachment" iconFirst="true" type="Transparent" enabled="true">
                        <layoutData>
                            <OverflowToolbarLayoutData moveToOverflow="true"/>
                        </layoutData>
                    </Button>
                    <Button text="PDF" icon="sap-icon://download" iconFirst="true" type="Emphasized" enabled="true">
                        <layoutData>
                            <OverflowToolbarLayoutData moveToOverflow="true"/>
                        </layoutData>
                    </Button>
                </uxap:actions>
                <uxap:breadCrumbsLinks/>
            </uxap:ObjectPageHeader>
        </uxap:headerTitle>
        <uxap:headerContent>

            <VBox>
                <items>
                    <Title text="{fornecedor/ID}" margin-right="0.5rem"/>
                    <Text text="{fornecedor/Nome}" class="sapMTitleStyleH3" press="_onFioriObjectPageHeaderContactPress"/>
                </items>
            </VBox>
            <VBox>
                <items>
                    <Title text="Bonificação"/>
                    <ObjectNumber number="{
                        path: 'Bonificacao',
                        type: 'sap.ui.model.odata.type.Decimal',
                        constraints: { scale: 2 }
                        }" unit="BRL" state="Success"/>
                </items>
            </VBox>
            <VBox>
                <items>
                    <Title text="Status"/>
                    <ObjectNumber number="{status/Descricao}" unit="" state="Success"/>
                </items>
            </VBox>
        </uxap:headerContent>
        <uxap:sections>
            <uxap:ObjectPageSection title="Informações Gerais">
                <uxap:subSections>
                    <uxap:ObjectPageSubSection title="Informações Gerais">
                        <uxap:blocks>
                            <layout:Grid defaultSpan="XL12 L12 M12 S12">
                                <layout:content>
                                    <form:Form>
                                        <form:layout>
                                            <form:ResponsiveGridLayout labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="4" columnsL="3" columnsM="2" singleContainerFullSize="false"/>
                                        </form:layout>
                                        <form:formContainers>
                                            <form:FormContainer title="">
                                                <form:formElements>
                                                    <form:FormElement>
                                                        <form:label>
                                                            <Label text="Descrição" required="false"/>
                                                        </form:label>
                                                        <form:fields>
                                                            <Input value="{Descricao}" placeholder="Indique aqui alguma descrição" type="Text" showValueHelp="false" enabled="true" visible="true" width="auto" valueHelpOnly="false" maxLength="0"/>
                                                        </form:fields>
                                                    </form:FormElement>
                                                    <form:FormElement>
                                                        <form:label>
                                                            <Label text="Período de Apuração" required="false"/>
                                                        </form:label>
                                                        <form:fields>
                                                            <DateRangeSelection
                                                                dateValue="{ApuracaoDe}"
                                                                secondDateValue="{ApuracaoAte}"
                                                                displayFormat="medium"
                                                            />
                                                        </form:fields>
                                                    </form:FormElement>
                                                </form:formElements>
                                            </form:FormContainer>
                                            <form:FormContainer title="">
                                                <form:formElements>
                                                    <form:FormElement>
                                                        <form:label>
                                                            <Label text="Fornecedor"/>
                                                        </form:label>
                                                        <form:fields>
                                                            <Input value="{fornecedor/Nome}" enabled="false"/>
                                                        </form:fields>
                                                    </form:FormElement>
                                                    <form:FormElement>
                                                        <form:label>
                                                            <Label text="Negociador"/>
                                                        </form:label>
                                                        <form:fields>
                                                            <Input value="{Negociador}"/>
                                                        </form:fields>
                                                    </form:FormElement>
                                                </form:formElements>
                                            </form:FormContainer>
                                            <form:FormContainer title="">
                                                <form:formElements>
                                                    <form:FormElement>
                                                        <form:label>
                                                            <Label text="Cláusula" required="false"/>
                                                        </form:label>
                                                        <form:fields>
                                                            <Input
                                                                id="clausulaInput"
                                                                value="{ClausulaID}"
                                                                showSuggestion="true"
                                                                suggest="suggestClausula"
                                                                suggestionItems="{/ClausulaSet}"
                                                                maxSuggestionWidth="300px"
                                                                showValueHelp="false">
                                                                <core:ListItem text="{ID}" additionalText="{Descricao}" />
                                                                <layoutData>
                                                                    <layout:GridData span="XL2 L3 M3 S4" />
                                                                </layoutData>
                                                            </Input>
                                                            <Input value="{clausula/Descricao}" enabled="false" required="true"/>
                                                        </form:fields>
                                                    </form:FormElement>
                                                </form:formElements>
                                            </form:FormContainer>
                                        </form:formContainers>
                                    </form:Form>
                                </layout:content>
                            </layout:Grid>
                        </uxap:blocks>
                    </uxap:ObjectPageSubSection>
                </uxap:subSections>
            </uxap:ObjectPageSection>
            <uxap:ObjectPageSection title="Abrangência">
                <uxap:subSections>
                    <uxap:ObjectPageSubSection title="Abrangência">
                        <uxap:blocks>
                            <layout:Grid defaultSpan="XL12 L12 M12 S12">
                                <layout:content>
