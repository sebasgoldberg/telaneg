                                    <Column width="auto" minScreenWidth="Phone">
                                        <header>
                                            <Text text="Centro/UF" width="auto"/>
                                        </header>
                                    </Column>
                                    <Column width="auto" minScreenWidth="Tablet" demandPopin="true">
                                        <header>
                                            <Text text="Descrição" width="auto"/>
                                        </header>
                                    </Column>
                                </columns>
                                <items>
                                    <ColumnListItem type="Active">
                                        <cells>
                                            <ObjectIdentifier title="BVRJ" text=""/>
                                            <ObjectStatus text="REFERENCIA RJ"/>
                                        </cells>
                                    </ColumnListItem>
                                </items>
                            </Table>
                            <layout:Grid defaultSpan="XL12 L12 M12 S12">
                                <layout:content>
                                    <form:Form>
                                        <form:layout>
                                            <form:ResponsiveGridLayout labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="4" columnsL="3" columnsM="2" singleContainerFullSize="false"/>
                                        </form:layout>
                                        <form:formContainers>
                                            <form:FormContainer title="">
                                                <form:formElements>
                                                    <form:FormElement>
                                                        <form:label>
                                                            <Label text="" required="false"/>
                                                        </form:label>
                                                        <form:fields>
                                                            <Text/>
                                                        </form:fields>
                                                    </form:FormElement>
                                                </form:formElements>
                                            </form:FormContainer>
                                        </form:formContainers>
                                    </form:Form>
                                </layout:content>
                            </layout:Grid>
                        </uxap:blocks>
                    </uxap:ObjectPageSubSection>
                </uxap:subSections>
            </uxap:ObjectPageSection>
